{
  "name": "lexipop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "export NEXT_PUBLIC_GIT_HASH=$(git rev-parse --short HEAD 2>/dev/null || echo 'dev') && prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx src/scripts/seed-database.ts",
    "envio:install": "npm install -g envio",
    "envio:init": "envio init",
    "envio:codegen": "envio codegen",
    "envio:dev": "envio dev",
    "envio:start": "envio start"
  },
  "dependencies": {
    "@farcaster/auth-kit": "^0.8.1",
    "@farcaster/miniapp-sdk": "^0.2.0",
    "@farcaster/miniapp-wagmi-connector": "^1.1.0",
    "@neynar/nodejs-sdk": "^3.34.0",
    "@prisma/client": "^6.17.1",
    "@rainbow-me/rainbowkit": "^2.0.8",
    "@tanstack/react-query": "^5.90.2",
    "@vercel/analytics": "^1.5.0",
    "framer-motion": "^12.23.24",
    "next": "15.5.4",
    "prisma": "^6.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "viem": "^2.17.11",
    "wagmi": "^2.18.0"
