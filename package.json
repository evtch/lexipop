    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "ignore-loader": "^0.1.2",
    "lightningcss": "^1.28.1",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "optionalDependencies": {
    "lightningcss-linux-x64-gnu": "^1.28.1",
